{
  "metadata": [
    {
      "src": [
        {
          "cwd": "..",
          "files": [
            "Elements/src/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "toc.yml",
          "*.md",
          "api/**",
          "Elements/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": [
          "favicon.ico"
        ]
      },
      {
        "files": [
          "js/**"
        ]
      },
      {
        "src": "../Elements/test/bin/debug/netcoreapp3.1/models/",
        "files": [
          "Elements_**.glb"
        ],
        "dest": "models"
      }
    ],
    "overwrite": [
      {
        "files": [],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "dest": "../docs/",
    "globalMetadata": {
      "_appTitle": "Hypar Docs",
      "_appFooter": "Copyright Â© Hypar Inc., 2020",
      "_disableContribution": true,
      "_enableSearch": true,
      "_disableBreadcrumb": true,
      "_disableFooter": true,
      "_appFaviconPath": "favicon.ico"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "./template/hypar"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}
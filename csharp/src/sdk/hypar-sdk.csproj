<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>Hypar.SDK</AssemblyName>
    <Title>Hypar SDK</Title>
    <Authors>Hypar</Authors>
    <PackageId>HyparSDK</PackageId>
    <Description>Function Execution for AEC</Description>
    <Summary>The Hypar SDK allows developers to create functions which runs on the Hypar platform.</Summary>
    <Copyright>Hypar LLC, 2018</Copyright>
    <PackageProjectUrl>https://www.hypar.io</PackageProjectUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Version>0.0.2-alpha8</Version>
    <Tags>AEC</Tags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="glTF2Loader" Version="1.1.1-alpha" />
    <PackageReference Include="NetTopologySuite" Version="1.15.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="Unofficial.LibTessDotNet" Version="2.0.0" />
  </ItemGroup>

</Project>
